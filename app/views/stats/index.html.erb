<h1>Listing stats</h1>

<table class="table">
  <tr>
    <th>Ip</th>
    <th>Os</th>
    <th>Browser</th>
    <th>From</th>
    <th>접속시간</th>
  </tr>

<% @stats.each do |stat| %>
  <tr>
    <td><%= stat.ip %></td>
    <td><%= stat.os %></td>
    <td><%= stat.browser %></td>
    <td><%= stat.from %></td>
    <td><%= stat.created_at.localtime %></td>
  </tr>
<% end %>
</table>

<br />
